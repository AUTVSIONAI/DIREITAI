# Frontend Environment Variables for Production
# Copy this file to .env.local and fill in your values

# API Configuration
# URL do backend em produção na Vercel
VITE_API_URL=https://direitai-backend.vercel.app/api

# Supabase Configuration (Frontend)
# Estas são as chaves públicas do Supabase
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Stripe Configuration (Frontend)
# Esta é a chave pública do Stripe
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# App Configuration
VITE_APP_NAME=DireitaAI
VITE_APP_URL=https://direitai.vercel.app

# Analytics (opcional)
# VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
# VITE_HOTJAR_ID=your_hotjar_id_here

# Feature Flags (opcional)
# VITE_ENABLE_ANALYTICS=true
# VITE_ENABLE_CHAT=true
# VITE_ENABLE_PAYMENTS=true

# Development URLs (para referência)
# VITE_API_URL=http://localhost:5120/api
# VITE_APP_URL=http://localhost:5173